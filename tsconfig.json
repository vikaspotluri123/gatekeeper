{
	"include": ["global.d.ts", "index.js", "pong.js", "lib/**/*.js"],
	"compilerOptions": {
		"strict": false,
		"forceConsistentCasingInFileNames": true,
		"downlevelIteration": true,
		"esModuleInterop": true,
		"module": "node16",
		"moduleResolution": "node16",
		"target": "es2022",
		"noEmit": true,
		"checkJs": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true
	},
}
